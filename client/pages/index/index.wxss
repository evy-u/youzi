/* pages/index/index.wxss */
page{
  width: 100%;
  height: 100%;
  background: rgb(240, 240, 240);
  font-size: 24rpx;
}
.container{
  height: 100%;
  width: 94%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
}
.search{
  width: 100%;
  height: 64rpx;
  margin: 20rpx 0;
  background: #fff;
  text-align: center;
  position: relative;
}
.search i{
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -60rpx;
  margin-top: -10rpx;
  z-index: 999;
  font-size: 20rpx
}

